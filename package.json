{
  "name": "@adeon/form",
  "version": "1.0.0",
  "description": "Smart React form component written with hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdeonMaster/adeon-form.git"
  },
  "homepage": "https://adeonmaster.github.io/adeon-form/",
  "bugs": {
    "url": "https://github.com/AdeonMaster/adeon-form/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "adeon"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "esnext": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "build:lib": "babel src/form --out-dir lib --ignore src/form/__tests__/",
    "build:es": "cross-env BABEL_ENV=es babel src/form --out-dir es --ignore src/form/__tests__/",
    "build": "npm run eslint && npm run jest && npm run build:lib && npm run build:es",
    "build-example:watch": "webpack -w --config config/webpack/dev.config.js",
    "build-example:dev": "webpack --config config/webpack/dev.config.js",
    "build-example": "webpack --config config/webpack/prod.config.js",
    "server": "node server/http.js",
    "server:ssl": "node server/https.js",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "jest": "jest --config config/jest/jest.config.json",
    "jest:coverage": "jest --config config/jest/jest.config.json --coverage"
  },
  "author": "AdeonMaster",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  }
}
