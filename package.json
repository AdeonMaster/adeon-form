{
  "name": "@adeon/form",
  "version": "1.0.1",
  "description": "Smart React form component written with hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdeonMaster/adeon-form.git"
  },
  "homepage": "https://adeonmaster.github.io/adeon-form/",
  "bugs": {
    "url": "https://github.com/AdeonMaster/adeon-form/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "adeon"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "esnext": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "build:lib": "babel src/form --out-dir lib --ignore src/form/__tests__/",
    "build:es": "cross-env BABEL_ENV=es babel src/form --out-dir es --ignore src/form/__tests__/",
    "build": "npm run eslint && npm run jest && npm run build:lib && npm run build:es",
    "build-example:watch": "webpack -w --config config/webpack/dev.config.js",
    "build-example:dev": "webpack --config config/webpack/dev.config.js",
    "build-example": "webpack --config config/webpack/prod.config.js",
    "server": "node server/http.js",
    "server:ssl": "node server/https.js",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "jest": "jest --config config/jest/jest.config.json",
    "jest:watch": "jest --config config/jest/jest.config.json --watch",
    "jest:coverage": "jest --config config/jest/jest.config.json --coverage"
  },
  "author": "AdeonMaster",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/react-hooks": "^3.3.0",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-sonarjs": "^0.5.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^9.0.2",
    "terser-webpack-plugin": "^3.0.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  }
}
